# Streamlit Cloud Secrets Configuration Template
# Copy this file to secrets.toml and fill in your actual values
# For local development, you can use .env file instead
# For Streamlit Cloud, add these secrets in the app settings

# Google Generative AI API Key (required)
GOOGLE_API_KEY = "your-google-api-key-here"

# LangChain/LangSmith API Key (optional - for tracing/debugging)
LANGCHAIN_API_KEY = "your-langchain-api-key-here"

# LangChain Tracing (optional - set to "true" to enable tracing)
LANGCHAIN_TRACING = "false"

# LangChain Project Name (optional)
LANGCHAIN_PROJECT = "autogpt-agent-builder"

