# Streamlit Cloud Secrets Configuration Template
# Copy this file to secrets.toml and fill in your actual values
# For local development, you can use .env file instead
# For Streamlit Cloud, add these secrets in the app settings

# Google Generative AI API Key (required)
GOOGLE_API_KEY = "your-google-api-key-here"

# Langfuse Configuration (optional - for LLM observability and prompt management)
# Get your keys from https://cloud.langfuse.com
LANGFUSE_SECRET_KEY = "sk-lf-..."
LANGFUSE_PUBLIC_KEY = "pk-lf-..."
LANGFUSE_BASE_URL = "https://cloud.langfuse.com"  # EU region (default)
# LANGFUSE_BASE_URL = "https://us.cloud.langfuse.com"  # US region
LANGFUSE_PROJECT = "prompt-to-agent"

# LangChain/LangSmith API Key (optional - for additional tracing)
LANGCHAIN_API_KEY = "your-langchain-api-key-here"
LANGCHAIN_TRACING = "false"
LANGCHAIN_PROJECT = "prompt-to-agent"


